{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/usdc-transaction-indexer/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/usdc-transaction-indexer/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/usdc-transaction-indexer",
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "blockchain": {
      "type": "library",
      "root": "libs/blockchain",
      "entryFile": "index",
      "sourceRoot": "libs/blockchain/src",
      "compilerOptions": {
        "tsConfigPath": "libs/blockchain/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "indexer": {
      "type": "application",
      "root": "apps/indexer",
      "entryFile": "main",
      "sourceRoot": "apps/indexer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/indexer/tsconfig.app.json",
        "webpack": true,
        "webpackConfigPath": "apps/indexer/webpack.config.ts"
      }
    },
    "pruning": {
      "type": "library",
      "root": "libs/pruning",
      "entryFile": "index",
      "sourceRoot": "libs/pruning/src",
      "compilerOptions": {
        "tsConfigPath": "libs/pruning/tsconfig.lib.json"
      }
    },
    "usdc-transaction-indexer": {
      "type": "application",
      "root": "apps/usdc-transaction-indexer",
      "entryFile": "main",
      "sourceRoot": "apps/usdc-transaction-indexer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/usdc-transaction-indexer/tsconfig.app.json"
      }
    }
  }
}